    "@assets": "./assets",
    "@models": "./models",
    "@jobs": "./jobs",
    "@database": "./database"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.4.1",
    "@discordjs/voice": "^0.9.0",
    "axios": "^0.27.2",
    "cron": "^2.0.0",
    "discord-api-types": "^0.33.0",
    "discord.js": "^13.7.0",
    "ffmpeg-static": "^5.0.0",
    "fs": "^0.0.1-security",
    "git-tag-version": "^1.3.1",
    "libsodium-wrappers": "^0.7.10",
    "module-alias": "^2.2.2",
    "path": "^0.12.7",
    "pretty-ms": "^7.0.1",
    "sequelize": "^6.19.2",
    "sqlite3": "^5.0.8",
    "ytdl-core": "^4.11.0",
    "ytsr": "^3.8.0"
  }
