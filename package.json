{
  "name": "bishop",
  "version": "4.0.2",
  "description": "Self-hosted Discord bot written in Node.js using Discord.js",
  "main": "app.js",
  "scripts": {
    "unregister": "node unregister-commands.js",
    "merge": "node merge-dependencies.js & npm i",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "start:dev": "nodemon app.js",
    "start:prod": "node unregister-commands.js && node app.js"
  },
  "author": "Alex Riley",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "_moduleAliases": {
    "@root": ".",
    "@backup": "./backup",
    "@config": "./config",
    "@models": "./models",
    "@helpers": "./helpers",
    "@database": "./database"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.5",
    "@discordjs/rest": "^1.7.1",
    "@discordjs/voice": "^0.16.0",
    "axios": "^0.27.2",
    "canvas": "^2.11.2",
    "cron": "^2.4.1",
    "discord-api-types": "^0.33.5",
    "discord.js": "^14.13.0",
    "git-tag-version": "^1.3.1",
    "libsodium-wrappers": "^0.7.11",
    "module-alias": "^2.2.3",
    "path": "^0.12.7",
    "pretty-ms": "^7.0.1",
    "semver": "^7.5.4",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0"
  },
  "workspaces": [
    "./modules/*"
  ]
}
