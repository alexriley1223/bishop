{
  "name": "discord-bot",
  "version": "3.0.2",
  "description": "Discord bot using Discord.js for D3X server.",
  "main": "app.js",
  "scripts": {
    "register": "node deploy-commands.js",
    "unregister": "node unregister-commands.js",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "start:prod": "node app.js",
    "start:dev": "nodemon app.js"
  },
  "author": "Alex Riley",
  "license": "ISC",
  "_moduleAliases": {
    "@root": ".",
    "@config": "./config",
    "@assets": "./assets",
    "@models": "./models",
    "@jobs": "./jobs",
    "@database": "./database"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.0",
    "@discordjs/builders": "^1.6.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.7.0",
    "@discordjs/voice": "^0.16.0",
    "axios": "^0.27.2",
    "canvas": "^2.9.3",
    "cron": "^2.0.0",
    "discord-api-types": "^0.33.0",
    "discord-player": "^6.6.1",
    "discord.js": "^14.0.0",
    "ffmpeg-static": "^5.0.0",
    "fs": "^0.0.1-security",
    "git-tag-version": "^1.3.1",
    "libsodium-wrappers": "^0.7.10",
    "module-alias": "^2.2.2",
    "path": "^0.12.7",
    "pretty-ms": "^7.0.1",
    "semver": "^7.5.4",
    "sequelize": "^6.19.2",
    "sqlite3": "^5.0.8",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0"
  }
}
