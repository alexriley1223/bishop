{
  "name": "discord-bot",
  "version": "2.1.1",
  "description": "Discord bot using Discord.js for D3X server.",
  "main": "app.js",
  "scripts": {
    "preupgrade": "echo \"Pulling latest changes\" & git pull",
    "upgrade": "echo \"Upgrading bot dependencies.\" & npm install && echo \"Syncing Database\" & node initialize-database.js && echo \"Registering new commands\" & node deploy-commands.js",
    "postupgrade": "echo \"Update complete. Please check if anything needs to be updated with config files\""
  },
  "author": "klingebot",
  "license": "ISC",
  "_moduleAliases": {
    "@root":  ".",
    "@config":  "./config",
    "@assets":  "./assets",
    "@models":  "./models",
    "@jobs":    "./jobs",
    "@database": "./database"
  },
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "axios": "^0.21.4",
    "cron": "^1.8.2",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "fs": "^0.0.1-security",
    "git-tag-version": "^1.3.1",
    "libsodium-wrappers": "^0.7.9",
    "module-alias": "^2.2.2",
    "path": "^0.12.7",
    "pretty-ms": "^7.0.1",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2",
    "ytdl-core": "^4.9.1",
    "ytsr": "^3.5.3"
  }
}
